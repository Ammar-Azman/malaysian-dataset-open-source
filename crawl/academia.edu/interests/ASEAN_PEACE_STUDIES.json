[{"id": 5513344, "title": "ASEAN and the South China Sea disputes: the search for peace and stability through security cooperation.", "created_at": "2013-12-23T14:51:43.123-08:00", "url": "https://www.academia.edu/5513344/ASEAN_and_the_South_China_Sea_disputes_the_search_for_peace_and_stability_through_security_cooperation?f_ri=409924", "dom_id": "work_5513344", "summary": "The South China Sea has sea lanes of global significance, rich fishing grounds, and potentially vast oil and gas reserves. ASEAN members are in conflict over China\u2019s claims in the area, but are working towards a binding code of conduct. Unity within ASEAN and continued diplomatic efforts will be essential to preventing disputes leading to armed conflict. By solving its internal inconsistencies and achieving stability, ASEAN will be able to promote its centrality in the region\u2019s security architecture as well as its importance in the global arena.", "downloadable_attachments": [{"id": 34789457, "asset_id": 5513344, "asset_type": "Work", "always_allow_download": false}], "ordered_authors": [{"id": 790079, "first_name": "Alessio", "last_name": "Fratticcioli", "domain_name": "umonash-my", "page_name": "AlessioFratticcioli", "display_name": "Alessio Fratticcioli", "profile_url": "https://umonash-my.academia.edu/AlessioFratticcioli?f_ri=409924", "photo": "https://0.academia-photos.com/790079/488848/6265962/s65_alessio.fratticcioli.jpg"}], "research_interests": [{"id": 797, "name": "International Relations", "url": "https://www.academia.edu/Documents/in/International_Relations?f_ri=409924", "nofollow": false}, {"id": 1031, "name": "Southeast Asian Studies", "url": "https://www.academia.edu/Documents/in/Southeast_Asian_Studies?f_ri=409924", "nofollow": false}, {"id": 3705, "name": "International Security", "url": "https://www.academia.edu/Documents/in/International_Security?f_ri=409924", "nofollow": false}, {"id": 5523, "name": "Security Studies", "url": "https://www.academia.edu/Documents/in/Security_Studies?f_ri=409924", "nofollow": false}, {"id": 6746, "name": "Southeast Asia", "url": "https://www.academia.edu/Documents/in/Southeast_Asia?f_ri=409924"}, {"id": 8785, "name": "Philippines", "url": "https://www.academia.edu/Documents/in/Philippines?f_ri=409924"}, {"id": 12108, "name": "China", "url": "https://www.academia.edu/Documents/in/China?f_ri=409924"}, {"id": 16655, "name": "Vietnam", "url": "https://www.academia.edu/Documents/in/Vietnam?f_ri=409924"}, {"id": 38972, "name": "ASEAN", "url": "https://www.academia.edu/Documents/in/ASEAN?f_ri=409924"}, {"id": 82703, "name": "South China Sea", "url": "https://www.academia.edu/Documents/in/South_China_Sea?f_ri=409924"}, {"id": 183140, "name": "ASEAN Studies", "url": "https://www.academia.edu/Documents/in/ASEAN_Studies?f_ri=409924"}, {"id": 259437, "name": "Regional Security in Southeast Asia", "url": "https://www.academia.edu/Documents/in/Regional_Security_in_Southeast_Asia?f_ri=409924"}, {"id": 409924, "name": "ASEAN PEACE STUDIES", "url": "https://www.academia.edu/Documents/in/ASEAN_PEACE_STUDIES?f_ri=409924"}, {"id": 430717, "name": "The South China Sea dispute", "url": "https://www.academia.edu/Documents/in/The_South_China_Sea_dispute?f_ri=409924"}, {"id": 692049, "name": "ASEAN Political Security Community", "url": "https://www.academia.edu/Documents/in/ASEAN_Political_Security_Community?f_ri=409924"}]}, {"id": 6925201, "title": "Barriers toward peace in southern Thailand", "created_at": "2014-04-30T19:55:03.692-07:00", "url": "https://www.academia.edu/6925201/Barriers_toward_peace_in_southern_Thailand?f_ri=409924", "dom_id": "work_6925201", "summary": "The history of talks with former-separatists since 2004 and recent government attempts at building trust with the local population have yet to achieve any noticeable results in troubled southern Thailand. Discussions surrounding autonomy or devolution for the south are hampered by political taboos and an inflexible Bangkok-based political elite. Certain local and external factors also shape the conflict, such as the role of Malaysia as a safe-haven for insurgents and the role of the black-economy in the region. A lack of communication, the inflexibility of the Thai political elite and the above-mentioned local and external factors all serve as barriers toward peace.", "downloadable_attachments": [{"id": 65147003, "asset_id": 6925201, "asset_type": "Work", "always_allow_download": false}], "ordered_authors": [{"id": 11002720, "first_name": "Gerard", "last_name": "McDermott", "domain_name": "cityu-hk", "page_name": "GerMcDer", "display_name": "Gerard McDermott", "profile_url": "https://cityu-hk.academia.edu/GerMcDer?f_ri=409924", "photo": "https://0.academia-photos.com/11002720/3268019/77363197/s65_gerard.mcdermott.jpg"}], "research_interests": [{"id": 797, "name": "International Relations", "url": "https://www.academia.edu/Documents/in/International_Relations?f_ri=409924", "nofollow": false}, {"id": 1031, "name": "Southeast Asian Studies", "url": "https://www.academia.edu/Documents/in/Southeast_Asian_Studies?f_ri=409924", "nofollow": false}, {"id": 1198, "name": "Peace and Conflict Studies", "url": "https://www.academia.edu/Documents/in/Peace_and_Conflict_Studies?f_ri=409924", "nofollow": false}, {"id": 1234, "name": "Thai Studies", "url": "https://www.academia.edu/Documents/in/Thai_Studies?f_ri=409924", "nofollow": false}, {"id": 2570, "name": "Religion and Thai Culture", "url": "https://www.academia.edu/Documents/in/Religion_and_Thai_Culture?f_ri=409924"}, {"id": 3059, "name": "Religion and Politics", "url": "https://www.academia.edu/Documents/in/Religion_and_Politics?f_ri=409924"}, {"id": 3849, "name": "Conflict", "url": "https://www.academia.edu/Documents/in/Conflict?f_ri=409924"}, {"id": 4424, "name": "Post-conflict Reconstruction and Development", "url": "https://www.academia.edu/Documents/in/Post-conflict_Reconstruction_and_Development?f_ri=409924"}, {"id": 5709, "name": "Politics", "url": "https://www.acad